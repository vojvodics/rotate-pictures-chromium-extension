{
  "name": "Image rotator",
  "description": "Rotates the image by specified angle",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "img.png"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "img.png",
    "default_popup": "popup.html",
    "default_title": "this is a title"
  },
  "permissions": ["tabs"]
}